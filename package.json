{
  "name": "ta-math",
  "version": "0.3.13",
  "description": "Technical analysis library in javascript. Calculates indicators and overlays from ohlcv data",
  "main": "dist/ta-math.cjs.js",
  "module": "dist/ta-math.module.js",
  "browser": "dist/ta-math.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "examples": "open-cli docs/index.html",
    "ci": "npm run build && npm run test",
    "test": "nyc mocha ./coverage/test.js",
    "coverage": "nyc report --reporter=lcov",
    "expected": "python ./test/expected.py"
  },
  "eslintIgnore": [
    "dist",
    "lib"
  ],
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@joseph184/rollup-plugin-node-builtins": "2.1.4",
    "codecov": "3.5.0",
    "mocha": "6.2.0",
    "nyc": "14.1.1",
    "open-cli": "5.0.0",
    "rollup": "1.20.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-istanbul": "2.0.1"
  },
  "keywords": [
    "technical-indicators",
    "technical-analysis",
    "quantitative-finance",
    "correlation-matrix",
    "finance"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/munrocket/ta-math.git"
  },
  "author": "munrocket",
  "license": "MIT"
}
